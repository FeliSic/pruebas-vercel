{
  "name": "proyecto2",
  "version": "1.0.0",
  "main": "index.js",
  "module": "esnext",
  "license": "MIT",
  "scripts": {
    "dev:backend": "ts-node-dev --respawn --transpile-only --project tsconfig.backend.json be-server/server/server.ts",
    "dev:frontend": "webpack serve --config webpack.config.js --mode development --port 4000",
    "dev": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\"",
    "build": "yarn build:backend && yarn build:frontend",
    "build:frontend": "NODE_ENV=production tsc -p tsconfig.frontend.json && webpack --mode production",
    "build:backend": "tsc -p tsconfig.backend.json",
    "start": "node dist/server/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "dropzone": "^6.0.0-beta.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.6",
    "react-router-dom": "^7.9.6",
    "resend": "^6.6.0",
    "sequelize": "^6.37.7",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.5",
    "serve": "^14.2.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
